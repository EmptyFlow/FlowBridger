<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PublishAot>true</PublishAot>
		<NativeAot>enable</NativeAot>
		<PublishTrimmed>true</PublishTrimmed>
		<EnableTrimAnalyzer>true</EnableTrimAnalyzer>
		<InvariantGlobalization>true</InvariantGlobalization>
		<Version>1.0.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="EmbeddedScripts\Embedded.Cpp.DynamicLinking.js" />
	  <None Remove="EmbeddedScripts\Embedded.Cpp.RuntimeLoading.js" />
	  <None Remove="EmbeddedScripts\Embedded.CSharp.Import.js" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="EmbeddedScripts\Embedded.Cpp.DynamicLinking.js">
			<LogicalName>Embedded.Cpp.DynamicLinking</LogicalName>
		</EmbeddedResource>
		<EmbeddedResource Include="EmbeddedScripts\Embedded.Cpp.RuntimeLoading.js">
			<LogicalName>Embedded.Cpp.RuntimeLoading</LogicalName>
		</EmbeddedResource>
		<EmbeddedResource Include="EmbeddedScripts\Embedded.CSharp.Import.js">
			<LogicalName>Embedded.CSharp.Import</LogicalName>
		</EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Jint" Version="4.4.1" />
	</ItemGroup>

</Project>
